{% extends 'base.html.twig' %}

{% block title %}Agence
{% endblock %}

{% block body %}

	<header style="background-repeat:no-repeat; background-position:center; background-size:cover; background-image:url('https://cdn.pixabay.com/photo/2021/04/16/05/13/perth-web-design-6182568_1280.jpg')" class="container-fluid h-90">
		<h1 class="h1 text-white text-center">{{ agence.secteur }}</h1>
		<div class="row">
			<div class="text-white col-4 d-flex flex-column align-items-center justify-content-around">
				<img src="https://img.icons8.com/ios/50/000000/google-maps.png"/>
				<p>{{ agence.adresse }}</p>
				<p>{{ agence.postale }}</p>
				<p>{{ agence.ville }}</p>
			</div>
			<div class="col-4 text-white  d-flex flex-column align-items-center justify-content-around">
				<img src="https://img.icons8.com/ios/50/000000/apple-phone.png"/>
				<p>0606060606</p>

			</div>
			<div class="col-4 text-white  d-flex flex-column align-items-center justify-content-around">
				<img src="data:image/png;base64,"/>
				<p>email</p>

			</div>
		</div>
	</header>
	<section id="section-member" class="container">
		<div class="row">
			{% for collab in agence.collaborateur %}
				<div class="col-4">
					<div class="card" style="width: 18rem;">
						<div class="card-body">
							<h5 class="card-title">{{collab.nom}}
								-
								{{collab.prenom}}</h5>
							<h6 class="card-subtitle mb-2 text-muted">{{collab.telephone}}</h6>
							<p class="card-text">{{collab.email}}</p>
							<a href="#" class="card-link">Card link</a>
							<a href="#" class="card-link">Another link</a>
						</div>
					</div>
				</div>
			{% endfor %}
		</div>
	</section>
